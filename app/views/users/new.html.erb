<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_for @user, url: users_path, html: { multipart: true } do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username %>
  <%= form.label :password %>
  <%= form.password_field :password_digest %>
  <%= form.file_field :avatar %>
  <%= form.submit %>
<% end %>
